# This is an example file for local.properties
# Copy this to a file named "local.properties" and add your actual tokens
# The local.properties file should never be committed to git

# Standard Android SDK location - will be generated by Flutter
sdk.dir=/path/to/your/Android/sdk

# Mapbox Downloads Token (required for building with Mapbox SDK)
# Get this from your Mapbox account with DOWNLOADS:READ scope
MAPBOX_DOWNLOADS_TOKEN=your_secret_mapbox_downloads_token 